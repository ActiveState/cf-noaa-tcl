#!/usr/bin/env tclsh
# -*- tcl -*-
package require Tcl 8.5

puts "debug    [package require debug 1.0.5]"
puts "cf::noaa [package require cf::noaa]"
puts ""

set m "\x0a\x06\x6c\x65\x67\x61\x63\x79\x10\x05\x42\x9b\x01\x0a\x62\x55\x70\x64\x61\x74\x65\x64\x20\x61\x70\x70\x20\x77\x69\x74\x68\x20\x67\x75\x69\x64\x20\x33\x62\x30\x30\x62\x37\x32\x63\x2d\x34\x36\x36\x39\x2d\x34\x30\x63\x30\x2d\x39\x66\x31\x63\x2d\x61\x31\x34\x64\x34\x64\x62\x38\x30\x61\x61\x34\x20\x28\x7b\x22\x63\x6f\x6e\x73\x6f\x6c\x65\x22\x3d\x3e\x74\x72\x75\x65\x2c\x20\x22\x73\x74\x61\x74\x65\x22\x3d\x3e\x22\x53\x54\x41\x52\x54\x45\x44\x22\x7d\x29\x10\x01\x18\xa1\xa4\xbb\x83\xc8\x80\x9c\xe2\x13\x22\x24\x33\x62\x30\x30\x62\x37\x32\x63\x2d\x34\x36\x36\x39\x2d\x34\x30\x63\x30\x2d\x39\x66\x31\x63\x2d\x61\x31\x34\x64\x34\x64\x62\x38\x30\x61\x61\x34\x2a\x03\x41\x50\x49"

puts "Encoded ([string length $m] bytes):"
puts [debug hexl $m {> }]
puts ""

puts Decoded:
puts [debug pdict [cf noaa envelope unpack $m]]
puts ""
exit
